{
  "name": "ragie-ai-slack-connector",
  "version": "1.0.0",
  "description": "Ragie slack connector",
  "main": "dist/server.js",
  "type": "module",
 "scripts": {
  "build": "tsc && eslint 'src/**/*.ts'",
  "test": "jest",
  "start": "node --max-old-space-size=4096 dist/server.js",
  "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
  "lint": "eslint '**/*.ts'",
  "lint:fix": "eslint '**/*.ts' --fix",
  "prepare": "husky init"
},
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@slack/web-api": "^7.7.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "eslint": "^9.16.0",
    "eslint-plugin-vue": "^9.31.0",
    "express": "^4.21.1",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.15.0",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.7.8",
    "body-parser": "^1.20.3",
    "groq-sdk": "^0.8.0"
  }
}
